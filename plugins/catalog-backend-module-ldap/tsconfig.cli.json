{
  "extends": "@backstage/cli/config/tsconfig.json",
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "target": "ES5",
    "rootDir": "./src",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "incremental": false,
    "skipLibCheck": true,
    "noUnusedLocals": false
  },
  "include": ["."]
}
